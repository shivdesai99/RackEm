{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/src/server.js",
  "scripts": {
    "start": "NODE_ENV=production node dist/src/server.js",
    "dev": "NODE_ENV=development ts-node-dev --respawn src/server.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "migrate": "NODE_ENV=production npx knex --knexfile dist/knexfile.js migrate:latest",
    "seed": "NODE_ENV=production npx knex --knexfile dist/knexfile.js seed:run",
    "migrate:dev": "NODE_ENV=development npx knex --knexfile knexfile.ts migrate:latest",
    "seed:dev": "NODE_ENV=development npx knex --knexfile knexfile.ts seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-validator": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "pg": "^8.13.1",
    "pg-connection-string": "^2.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/knex": "^0.15.2",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1"
  }
}
